<section class="row full-width category-listing">
  <div class="container">
    <form id="filter_category" onsubmit="remove_empty_inputs(this); setCookie('applied_filter', true);"
      action="/motor/sparepart-motor" accept-charset="UTF-8" method="get"><input name="utf8" type="hidden" value="✓">
      <input type="hidden" name="id_url" id="id_url">
      <input type="hidden" name="keyword" id="key">
      <input type="hidden" name="category" id="cat" value="motor">
      <input type="hidden" name="newest" id="newest">
      <input type="hidden" name="is_hot" id="is_hot">
      <input type="hidden" name="lat" id="lat">
      <input type="hidden" name="lon" id="lon">
      <input type="hidden" name="ad_type" id="ad_type">
      <div class="category-listing__banner-top banner-left">
        <div id="jbanner0"></div>
      </div>

      <%= render "contents/list_category" %>

      <div class="col-9" id="product-col">
        <% if is_brand %>
          <%= render partial: "contents/filter_brand", 
            locals: { 
              products: products, 
              category: category
            } 
          %>
        <% else %>
          <%= render partial: "contents/filter", locals: { products: products, category: category } %>
        <% end %>
        <%= render partial: "static_pages/list_product", 
          locals: { 
            products: products,
            image_error: image_error,
            is_category: is_category
          }
        %>

        <div class="category-listing__pagination">
          <div class="col-12" id="pagination">
            <div class="row">
              <div class="pull-left">
                <div class="pagination" style="background: none; padding: 0">
                  <span class="previous_page disabled">
                    <span class="prev-page">
                      <span class="icon icon_prev">
                      </span>
                    </span>
                  </span>
                  <em class="current">
                    1
                  </em>
                  <a rel="next" href="/motor/sparepart-motor/?page=2">
                    2
                  </a>
                  <a href="/motor/sparepart-motor/?page=3">
                    3
                  </a>
                  <a href="/motor/sparepart-motor/?page=4">
                    4
                  </a>
                  <a href="/motor/sparepart-motor/?page=5">
                    5
                  </a>
                  <a href="/motor/sparepart-motor/?page=6">
                    6
                  </a>
                  <a href="/motor/sparepart-motor/?page=7">
                    7
                  </a> <span class="gap">…</span> <a class="next_page" rel="next"
                    href="/motor/sparepart-motor/?page=2"><span class="next-page"><span
                        class="icon icon_next"></span></span></a></div>
              </div>
              <div class="pull-right">
                <%= content_tag(:span) %>
                <span>
                  page
                </span>
                <span class="page-number">
                  1
                </span>
                of
                <span class="orange">
                  717
                </span>
              </div>
            </div>
          </div>
        </div>
        <div class="category-listing__banner-bot" id="ad-bot">
          <div id="jbanner1"></div>
        </div>
      </div>
      <div class="col-1" style="width: 122px">
        <div class="category-listing__adsense-skyscraper sticky" id="sticky-ad" style="margin-top: 0px;">
          <script async="" src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
          <!-- Native Banner Adsense Desktop -->
          <ins class="adsbygoogle" data-ad-client="ca-pub-1720575921318265" data-ad-slot="7650473821"
            style="display:inline-block;width:120px;height:600px" data-adsbygoogle-status="done">
            <ins id="aswift_0_expand"
              style="display:inline-table;border:none;height:600px;margin:0;padding:0;position:relative;visibility:visible;width:120px;background-color:transparent;">
              <ins id="aswift_0_anchor"
                style="display:block;border:none;height:600px;margin:0;padding:0;position:relative;visibility:visible;width:120px;background-color:transparent;">
                <iframe width="120" height="600" frameborder="0" marginwidth="0" marginheight="0" vspace="0" hspace="0"
                  allowtransparency="true" scrolling="no" allowfullscreen="true" id="aswift_0" name="aswift_0"
                  style="left:0;position:absolute;top:0;border:0px;width:120px;height:600px;">
                </iframe>
              </ins>
            </ins>
          </ins>
          <script>
            (adsbygoogle = window.adsbygoogle || []).push({});
          </script>

        </div>
      </div>
    </form>

  </div>
</section>